<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyp.dao.DbTableMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.dyp.entity.DbTable">
		<id column="id" property="id" />
		<result column="father_uuid" property="fatherUuid" />
		<result column="data_source_id" property="dataSourceId" />
		<result column="code" property="code" />
		<result column="dbet_name" property="dbetName" />
		<result column="dbct_name" property="dbctName" />
		<result column="order_num" property="orderNum" />
		<result column="status" property="status" />
		<result column="remarks" property="remarks" />
		<result column="create_user_id" property="createUserId" />
		<result column="create_time" property="createTime" />
		<result column="update_time" property="updateTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, father_uuid AS fatherUuid, data_source_id AS dataSourceId, code, dbet_name AS dbetName, dbct_name AS dbctName, order_num AS orderNum, status, remarks, create_user_id AS createUserId, create_time AS createTime, update_time AS updateTime
    </sql>

</mapper>
